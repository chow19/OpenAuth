    <add name="KindeeDBContext" connectionString="Data Source=192.168.0.131;Initial Catalog=AIS20180601000543;Persist Security Info=True;User ID=sa;Password=sa@0750;MultipleActiveResultSets=True" providerName="System.Data.SqlClient" />
